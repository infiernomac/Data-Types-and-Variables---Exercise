using System;

namespace Sum_of_Chars
{
	class Program
	{
		static void Main(string[] args)
		{
			int number = int.Parse(Console.ReadLine());

			int sum = 0;

			for (int i = 0; i < number; i++)
			{
				char ascll = char.Parse(Console.ReadLine());

				sum += ascll;
			}

			Console.WriteLine($"The sum equals: {sum}");
		}
	}
}
